#main
	padding 30px 0 100px
	background-color: #fff
	background-image url(../img/backs/main-bg.png)
	background-repeat no-repeat
	background-position: center top
	background-size: cover
.main-container
	width 100%
	max-width 970px
	margin 0 auto
	background-color: rgba(#000,.7)
	padding 130px 70px 50px
	text-align: center
.main-title
	font-size: 36px;
	font-family:OS
	color: rgb(255, 224, 19);
	text-transform: uppercase;
	line-height: 1.667;
.main-subtitle
	font-size: 20px;
	font-family: ABd
	color: #fff
	line-height: 1.2;
	width 100%
	width: 601px;
	margin: 60px auto 76px