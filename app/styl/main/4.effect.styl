#effect
	padding 25px 0 40px
	position relative
	z-index 12
	background-color: #1A2437
	background-image url(../img/backs/ornament.png)
	background-repeat no-repeat
	background-position: center
	background-size: contain
	&:before
		content ''
		display block
		position: absolute
		width 100%
		height 83px
		top -83px
		left 0
		right 0
		background-image url(../img/backs/effect-top-bg.png)
		background-repeat no-repeat
		background-position: bottom center
		background-size: 100% 100%
	&:after
		content ''
		display block
		position: absolute
		width 100%
		height 100px
		bottom -100px
		left 0
		right 0
		background-image url(../img/backs/effect-bottom-bg.png)
		background-repeat no-repeat
		background-position: top center
		background-size: 100% 100%
.effect-list
	width 100%
	max-width 450px
	margin-bottom: 50px
	li
		list-style none
		margin-bottom: 30px
		padding-left: 75px
		min-height 58px
		background-image url(../img/icons/check.png)
		background-repeat no-repeat
		background-position: left top
		color #fbf1f9